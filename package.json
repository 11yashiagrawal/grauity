        "storybook-addon-code-editor": "^0.3.7",
        "style-loader": "^3.3.1",
        "svgo": "^3.0.2",
        "typescript": "^4.9.4",
        "webpack": "^5.75.0",
        "webpack-cli": "^5.0.1"
    },
