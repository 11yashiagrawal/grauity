        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-react": "^7.31.11",
        "fantasticon": "^2.0.0",
        "html-webpack-plugin": "^5.5.0",
        "http-server": "^14.1.1",
        "lint-staged": "^13.1.0",
        "mini-css-extract-plugin": "^2.7.2",
        "path": "^0.12.7",
        "pre-commit": "^1.2.2",
        "prettier": "2.8.1",
        "react": "^16.8.0 || ^17.0.0",
        "react-dom": "^16.8.0 || ^17.0.0",
