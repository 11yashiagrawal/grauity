{
    "name": "@newtonschool/grauity",
    "version": "0.0.14",
    "description": "",
    "keywords": [
        "Newton School",
        "Grauity",
        "GUI",
        "Design System",
        "React",
        "CSS"
    ],
    "homepage": "https://github.com/Newton-School/grauity#readme",
    "author": "Newton School",
    "contributors": [
        {
            "name": "Praduman Goyal",
            "email": "goyal.praduman611@gmail.com",
            "homepage": "https://github.com/pradumangoyal"
        },
        {
            "name": "Mihir Sachdeva",
            "email": "mihirsachdeva.ms@gmail.com",
            "homepage": "https://github.com/mihirsachdeva"
        }
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Newton-School/grauity.git"
    },
    "license": "GPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/Newton-School/grauity/issues"
    },
    "engines": {
        "npm": ">=5",
        "node": ">=8.15.1"
    },
    "source": "ui/index.ts",
    "main": "dist/main.cjs",
    "module": "dist/module.mjs",
    "types": "dist/index.d.ts",
    "targets": {
        "main": {
            "optimize": true
        },
        "module": {
            "optimize": true
        }
    },
    "files": [
        "CHANGELOG.md",
        "ui",
        "dist"
    ],
    "staticFiles": [
        {
            "staticPath": "ui/fonts",
            "staticOutPath": "fonts"
        }
    ],
    "typeRoots": [
        "../node_modules/@types"
    ],
    "scripts": {
        "build": "rm -r .parcel-cache && npm run build-icons && npm run build-lib",
        "build-lib": "npm run extract-typings && parcel build",
        "build-icons": "npm run build-icons:optimize && npm run build-icons:generate",
        "build-icons:optimize": "rimraf ./iconland/optimised/*.svg & svgo -q -p 8 -f ./iconland/seeds -o ./iconland/optimised",
        "build-icons:generate": "node scripts/generate-font-icons-from-optimised-svgs.cjs",
        "lint": "npm run lint:eslint -- .",
        "lint:fix": "npm run lint:eslint -- . --fix",
        "lint:eslint": "eslint --ignore-path .gitignore --ignore-path .eslintignore",
        "lint:eslint:fix": "eslint  --ignore-path .gitignore --ignore-path .eslintignore --fix",
        "lint:staged": "lint-staged",
        "prettify": "prettier --write",
        "extract-typings": "tsc --project tsconfig.types.json",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook -o docs && echo 'grauity.newtonschool.co' > docs/CNAME"
    },
    "pre-commit": "lint:staged",
    "lint-staged": {
        "*.js": [
            "npm run prettify",
            "npm run lint:eslint:fix"
        ],
        "*.ts": [
            "npm run prettify",
            "npm run lint:eslint:fix"
        ],
        "*.json": "npm run prettify"
    },
    "devDependencies": {
        "@babel/core": "^7.20.12",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.18.6",
        "@parcel/packager-ts": "^2.8.2",
        "@parcel/transformer-sass": "^2.8.2",
        "@parcel/transformer-typescript-tsc": "^2.8.2",
        "@parcel/transformer-typescript-types": "^2.8.2",
        "@storybook/addon-actions": "^6.5.15",
        "@storybook/addon-docs": "^6.5.15",
        "@storybook/addon-essentials": "^6.5.15",
        "@storybook/addon-interactions": "^6.5.15",
        "@storybook/addon-links": "^6.5.15",
        "@storybook/addons": "^6.5.15",
        "@storybook/builder-webpack5": "^6.5.15",
        "@storybook/manager-webpack5": "^6.5.15",
        "@storybook/react": "^6.5.15",
        "@storybook/testing-library": "^0.0.13",
        "@storybook/theming": "^6.5.15",
        "@types/node": "^18.11.18",
        "@types/react": "18.2.0",
        "@types/react-dom": "18.2.0",
        "@types/styled-components": "^5.1.26",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.48.0",
        "babel-loader": "^9.1.0",
        "css-loader": "^6.7.3",
        "eslint": "^7.32.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsx-a11y": "^6.6.1",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "fantasticon": "^2.0.0",
        "http-server": "^14.1.1",
        "lint-staged": "^13.1.0",
        "mini-css-extract-plugin": "^2.7.2",
        "parcel": "^2.8.2",
        "parcel-plugin-static-files-copy": "^2.6.0",
        "parcel-reporter-static-files-copy": "^1.5.0",
        "path": "^0.12.7",
        "pre-commit": "^1.2.2",
        "prettier": "2.8.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "rimraf": "^3.0.2",
        "sass": "^1.57.1",
        "sass-loader": "^13.2.0",
        "scss": "^0.2.4",
        "storybook": "^6.5.15",
        "storybook-addon-code-editor": "^0.3.7",
        "style-loader": "^3.3.1",
        "styled-components": "^5.3.6",
        "svgo": "^3.0.2",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "classnames": "^2.3.2",
        "lodash": "^4.17.21",
        "prop-types": "^15.8.1"
    },
    "peerDependencies": {
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "styled-components": "4.2.0"
    },
    "resolutions": {
        "@storybook/react/webpack": "^5"
    }
}
